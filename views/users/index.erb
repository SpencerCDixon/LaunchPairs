<h1> All Users Available </h1>
<table class="table">
  <tr>
    <th>Photo</th>
    <th>Name</th>
    <th>Username</th>
    <th>User ID</th>
    <th>Email</th>
  </tr>
  <% @users.each do |user| %>
  <tr>
    <td><img class="tiny-img" src="<%= user['avatar_url'] %>"></td>
    <td><%= user['name'] %></td>
    <td>@<%= user['username'] %></td>
    <td><%= user['uid'] %></td>
    <td><%= user['email'] %></td>
  </tr>
  <% end %>
</table>
